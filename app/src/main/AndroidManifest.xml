<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ryanjoshsean.kingofmountoliver">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainMenu"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HowManyPlayers"
            android:label="@string/title_activity_how_many_players"
            android:parentActivityName=".MainMenu">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ryanjoshsean.kingofmountoliver.MainMenu" />
        </activity>
        <activity
            android:name=".SelectCharacters"
            android:label="@string/title_activity_select_characters" />
        <activity
            android:name=".GameActivity"
            android:label="@string/title_activity_game"></activity>
    </application>

</manifest>
